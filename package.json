{
	"name": "codekasten",
	"displayName": "codekasten",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.51.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"*"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "codekasten.create-note",
				"title": "Codekasten: Create Note"
			},
			{
				"command": "codekasten.show-graph",
				"title": "Codekasten: Show graph"
			},
			{
				"command": "codekasten.search-note",
				"title": "Codekasten: Search Note"
			}, {
				"command": "codekasten.update-graph", 
				"title": "Codekasten: Update Graph"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"lint": "eslint src --ext ts",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"test": "node ./out/test/runTest.js",
		"unittest": "mocha"
	},
	"devDependencies": {
		"@types/chai": "^4.2.14",
		"@types/glob": "^7.1.1",
		"@types/mocha": "^7.0.2",
		"@types/node": "^13.11.0",
		"@types/vscode": "^1.51.0",
		"@typescript-eslint/eslint-plugin": "^2.30.0",
		"@typescript-eslint/parser": "^2.30.0",
		"chai": "^4.2.0",
		"eslint": "^6.8.0",
		"glob": "^7.1.6",
		"mocha": "^7.1.2",
		"typescript": "^3.8.3",
		"vscode-test": "^1.3.0"
	},
	"dependencies": {
		"@types/graphlib": "^2.1.7",
		"graphlib": "^2.1.8"
	}
}
